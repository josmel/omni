/**
* Layout CSS
* @class Layout
* @module layout.css
*/
body
	background #F8F8F8
	cursor default
	font-family family1
	height 100%
	margin 0
	padding 0
h1, h2,h3,h4
	margin 0
p
	margin 0
a
	color inherit
	text-decoration none
ul
	list-style none
	margin 0
	padding 0
button
	border none
	padding 0
iframe
	display block
fieldset
	border none
	margin 0
	padding 0
dl
	margin 0
	padding 0
dt
	float left
	padding 4px 0
	text-align right
	width 96px
dd
	margin-left 112px
	padding 4px 0
input[readonly="readonly"]
	background #FFF !important
	cursor default